<h1>Current Menu:</h1>
{{#each dishes as |dish|}}
<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="{{dish.url}}" alt="{{ dish.name }}">
  <div class="card-body">
    <h5 class="card-title">{{dish.name}}</h5>
    <p class="card-text">{{ dish.description }}</p>
    <a href="#" class="btn btn-primary">Edit Dish</a>
  </div>
</div>
{{/each}}

